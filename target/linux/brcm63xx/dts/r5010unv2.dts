/dts-v1/;

#include "bcm6328.dtsi"

#include <dt-bindings/input/input.h>

/ {
	model = "NuCom R5010UN v2";
	compatible = "nucom,r5010unv2", "brcm,bcm6328";

	chosen {
		bootargs = "root=/dev/mtdblock2 rootfstype=squashfs,jffs2 noinitrd console=ttyS0,115200";
	};

	
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		debounce-interval = <60>;

		reset {
			label = "reset";
			gpios = <&gpio0 23 1>;
			linux,code = <KEY_RESTART>;
		};
		wps {
			label = "wps";
			gpios = <&gpio0 24 1>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		inet_green {
			label = "R5010UNv2:green:inet";
			gpios = <&gpio0 1 1>;
		};
		inet_fail_red {
			label = "R5010UNv2:red:inet-fail";
			gpios = <&gpio0 2 1>;
		};
		dsl_red {
			label = "R5010UNv2:green:dsl";
			gpios = <&gpio0 3 1>;
		};
		power_green {
			label = "R5010UNv2:green:power";
			gpios = <&gpio0 4 1>;
			default-state = "on";
		};
		power_fail_red {
			label = "R5010UNv2:red:power-fail";
			gpios = <&gpio0 5 1>;
		};
		wps_green {
			label = "R5010UNv2:green:wps";
			gpios = <&gpio0 10 1>;
		};
		usb_green {
			label = "R5010UNv2:green:usb";
			gpios = <&gpio0 11 1>;
		};
	};
};

