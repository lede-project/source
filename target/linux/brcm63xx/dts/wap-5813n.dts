/dts-v1/;

#include "bcm6368.dtsi"

#include <dt-bindings/input/input.h>

/ {
	model = "Comtrend WAP-5813n";
	compatible = "comtrend,wap-5813n", "brcm,bcm6368";

	chosen {
		bootargs = "root=/dev/mtdblock2 rootfstype=squashfs,jffs2 noinitrd console=ttyS0,115200";
		stdout-path = "serial0:115200n8";
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		debounce-interval = <60>;

		wlan {
			label = "wlan";
			gpios = <&pinctrl 32 1>;
			linux,code = <KEY_WLAN>;
		};
		reset {
			label = "reset";
			gpios = <&pinctrl 34 1>;
			linux,code = <KEY_RESTART>;
		};
		wps {
			label = "wps";
			gpios = <&pinctrl 35 1>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		inet_green {
			label = "WAP-5813n:green:inet";
			gpios = <&pinctrl 5 0>;
		};
		power_green {
			label = "WAP-5813n:green:power";
			gpios = <&pinctrl 22 0>;
			default-state = "on";
		};
		wps_green {
			label = "WAP-5813n:green:wps";
			gpios = <&pinctrl 23 1>;
		};
		power_red {
			label = "WAP-5813n:red:power";
			gpios = <&pinctrl 24 0>;
		};
		inet_red {
			label = "WAP-5813n:red:inet";
			gpios = <&pinctrl 31 0>;
		};
	};
};

&pflash {
	status = "ok";

	linux,part-probe = "bcm63xxpart";

	cfe@0 {
		label = "CFE";
		reg = <0x000000 0x010000>;
		read-only;
	};

	linux@10000 {
		label = "linux";
		reg = <0x010000 0x7e0000>;
	};

	nvram@7f0000 {
		label = "nvram";
		reg = <0x7f0000 0x010000>;
	};
};

&lsspi {
	switch@0 {
		compatible = "brcm,bcm53115";
		reg = <0>;
		spi-max-frequency = <781000>;

		lede,alias = "eth0";

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			lan@0 {
				reg = <0>;
				label = "lan4";
			};

			lan@1 {
				reg = <1>;
				label = "lan3";
			};

			lan@2 {
				reg = <2>;
				label = "lan2";
			};

			lan@3 {
				reg = <3>;
				label = "lan1";
			};

			wan@4 {
				reg = <4>;
				label = "wan";
			};

			cpu@5 {
				reg = <5>;
				label = "cpu";

				fixed-link {
					speed = <1000>;
					full-duplex;
					asym-pause;
					pause;
				};
			};
		};
	};
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pci>;
};

&uart0 {
	status = "ok";
};
