config wifi-device  radio0
	option type     mac80211
	option phy	phy0
	option channel  auto
	# TODO: CHANGE THE MAC-ADDRESS:
	option macaddr	00:11:22:33:44:55
	option hwmode	11g
#	option country	DE

	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1

config wifi-iface
	option device   radio0
	option powersave 1
	option network  lan
	option mode     sta
	option ssid     OpenWrt
#	option encryption psk2
#	option key	"foobar"
