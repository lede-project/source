/dts-v1/;

#include "ASL26555.dtsi"

/ {
	compatible = "alphanetworks,asl26555-16m", "alphanetworks,asl26555", "ralink,rt3050-soc";
	model = "Alpha ASL26555 (16M)";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "uboot";
			reg = <0x0 0x30000>;
			read-only;
		};

		partition@30000 {
			label = "uboot-env";
			reg = <0x30000 0x10000>;
			read-only;
		};

		partition@40000 {
			label = "factory";
			reg = <0x40000 0x10000>;
			read-only;
		};

		partition@50000 {
			label = "firmware";
			reg = <0x50000 0xf80000>;
		};

		partition@fd0000 {
			label = "cert";
			reg = <0xfd0000 0x10000>;
			read-only;
		};

		partition@fe0000 {
			label = "langpack";
			reg = <0xfe0000 0x10000>;
			read-only;
		};

		devdata: partition@ff0000 {
			label = "devdata";
			reg = <0xff0000 0x10000>;
			read-only;
		};
	};
};
