/dts-v1/;

#include "VGV7510KW22.dtsi"

/ {
	fpi@10000000 {
		localbus@0 {
			nor-boot@0 {
				partitions {
					partition@0 {
						label = "uboot";
						reg = <0x0 0x60000>; /* 384 KiB */
						read-only;
					};

					partition@60000 {
						label = "uboot-env";
						reg = <0x60000 0x20000>; /* 128 KiB */
						read-only;
					};

					partition@80000 {
						label = "firmware";
						reg = <0x80000 0xf60000>; /* 15744 KiB */
					};
				};
			};
		};
	};
};
