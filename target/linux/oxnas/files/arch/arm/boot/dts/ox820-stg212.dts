/dts-v1/;

#include "ox820.dtsi"

/ {
	model = "MitraStar Technology Corp. STG-212";

	chosen {
		bootargs = "console=ttyS0,115200n8 earlyprintk=serial mem=128M";
	};

	uart@44200000 {
		status = "okay";
	};

	sata@45900000 {
		status = "okay";
	};

	nand@41000000 {
		status = "okay";

		partition@0 {
			label = "boot";
			reg = <0x00000000 0x00e00000>;
			/*read-only;*/
		};

		partition@e00000 {
			label = "ubi";
			reg = <0x00e00000 0x07200000>;
		};
	};

	ethernet@40400000 {
		status = "okay";
	};

	ehci@40200100 {
		status = "okay";
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;

		reset {
			label = "reset";
			gpios = <&GPIOB 11 1>;
			linux,code = <0x198>;
		};
		copy {
			label = "copy";
			gpios = <&GPIOB 13 1>;
			linux,code = <0x85>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		status {
			label = "zyxel:blue:status";
			gpios = <&GPIOB 5 0>;
		};
		status2 {
			label = "zyxel:red:status";
			gpios = <&GPIOB 6 1>;
		};
		copy {
			label = "zyxel:orange:copy";
			gpios = <&GPIOB 8 1>;
		};
	};

	i2c-gpio {
		compatible = "i2c-gpio";
		gpios = <&GPIOB 9 0 &GPIOB 10 0>;
		i2c-gpio,delay-us = <10>;
	};

};
