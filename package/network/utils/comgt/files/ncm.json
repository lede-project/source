	"sony": {
		"initialize": [
			"AT+CFUN=1",
			"AT+CGDCONT=${profile},\\\"${pdptype}\\\"${apn:+,\\\"$apn\\\"}",
			"AT*EIAAUW=${profile},1,\\\"${username}\\\",\\\"${password}\\\",${auth:-00111}"
		],
		"modes": {
			"umts": "AT+CFUN=6",
			"gsm": "AT+CFUN=5"
		},
		"connect": "AT*ENAP=1,${profile}",
		"disconnect": "AT*ENAP=0"
	}
