#!/bin/sh /etc/rc.common
# (C) 2013 openwrt.org

START=40

boot() {
	/sbin/block mount
}

start() {
	echo "this file has been obsoleted. please call \"/sbin/block mount\" directly"
}

restart() {
	start
}

# Block umount on LuCI apply does bad things
reload() {
	/sbin/block mount
}

stop() {
	/sbin/block umount
}
